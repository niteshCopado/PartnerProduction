<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>48.0</apiVersion>
    <isExposed>true</isExposed>
    <masterLabel>Listing Detail</masterLabel>
    <targets>
        <target>lightning__AppPage</target>
        <target>lightning__RecordPage</target>
        <target>lightning__HomePage</target>
        <target>lightningCommunity__Page</target>
        <target>lightningCommunity__Default</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="lightningCommunity__Default">
            <property name="recordId" type="String" default="{!recordId}" label="Record Id" description="Automatically bind the page's record id to the component variable"/>
            <property name="communityname" type="String" default="CopadoExchange" label="Community Name" description="The name of the community for the download links to work (Case Sensitive)."/>
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>