        <picklistValues>
            <masterLabel>Related</masterLabel>
            <translation><!-- Related --></translation>
        </picklistValues>
    </fields>
    <fields>
        <help><!-- Applies a Discount Amount to the List Price to become the Sales Price value in Quote and Order configurator page. Applicable when Price Behavior is set to Discounted --></help>
        <label><!-- Price Discount --></label>
        <name>kugo2p__PriceDiscount__c</name>
    </fields>
    <fields>
        <help><!-- In Quote and Order Configurator page, Bundled will Lock Quantity and multiply the Default Quantity by the Configured Product quantity. Defined will Lock Quantity and maintain Default Quantity. Related or, if blank, will allow edits of Default Quantity --></help>
        <label><!-- Quantity Behavior --></label>
        <name>kugo2p__QuantityBehavior__c</name>
        <picklistValues>
