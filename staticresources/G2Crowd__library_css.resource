    list-style: none;
}

.G2Crowd ul.methods {
    padding: 0;
    list-style: none;
}

.G2Crowd ul.methods > li:first-child > *:first-child {
    padding-top: 0;
}

.G2Crowd [ng\:cloak], .G2Crowd [ng-cloak], .G2Crowd [data-ng-cloak], .G2Crowd [x-ng-cloak], .G2Crowd .ng-cloak, .G2Crowd .x-ng-cloak {
    display: none !important;
}

.G2Crowd .version-picker {
    background: #267ED5 !important;
    border-bottom: 1px solid rgba(0, 0, 0, 0.12);
    margin-bottom: 1px !important;
}

.G2Crowd .version-picker .menu-toggle-list {
    background: transparent !important;
}

.G2Crowd md-icon.block {
    color: #106CC8;
    display: block;
    height: 50px;
    width: 50px;
}

.G2Crowd .doc-demo-content {
    padding: 0;
}

.G2Crowd docs-demo .doc-demo-content {
    margin: 16px;
}

.G2Crowd .site-content-toolbar {
    background: #f6f6f6 !important;
    color: #666 !important;
    z-index: 3;
}

.G2Crowd .site-content-toolbar {
    background: #f6f6f6 !important;
    color: #202020 !important;
    z-index: 3;
}

.G2Crowd .service-desc {
    overflow: hidden;
    background: #E3ECF5;
    padding: 16px;
    margin: 0 0 8px;
}

.G2Crowd .service-desc > p:first-child {
    margin-top: 0;
}

.G2Crowd .service-desc > p:last-child {
    margin-bottom: 0;
}

@media screen and (-ms-high-contrast: active) {
    .G2Crowd a {
        text-decoration: underline;
    }
    .G2Crowd iframe, .G2Crowd hljs pre {
        border: 1px solid #fff;
    }
}

.G2Crowd .no-transition {
    transition: none !important;
}

.G2Crowd table.attributes, .G2Crowd table.md-api-table {
    padding-left: 15px;
    margin-bottom: 20px;
}

.G2Crowd table.attributes tr td:first-child {
    font-weight: bold;
    background-color: #DBEEF5;
}

.G2Crowd table.attributes tr td:last-child {
    padding-left: 10px;
}

.G2Crowd table.md-api-table:not(.md-css-table) tr td:first-child {
    font-weight: bold;
}

.G2Crowd table.md-css-table .md-css-selector {
    display: block;
    padding: 8px 16px;
    /* Offset the padding of the <td> element */
    margin: -12px -16px 12px -16px;
    background-color: #0C2238;
    color: #9ccc65;
}

.G2Crowd table.md-css-table td p {
    margin: 0.8em 0;
}

.G2Crowd .layout_note {
    font-size: 0.9em;
    margin: -5px 40px 0px 20px;
    color: #013972;
    background-color: rgba(156, 204, 101, 0.4);
    padding: 20px;
}

.G2Crowd .contributor_tables {
    padding-left: 25px;
}

.G2Crowd .contributor_tables > table {
    padding-top: 10px;
    padding-bottom: 10px;
}

@media (max-width: 500px) {
    .G2Crowd {
        /* Reduce the size of the nav logo/header */
        /* Reduce the padding around standard doc content */
        /* Reduce the padding around doc demos */
    }
    .G2Crowd .docs-logo > img {
        height: 75px;
        margin-top: 10px;
    }
    .G2Crowd .docs-logo h1 {
        font-size: 18px;
        line-height: 2rem;
    }
    .G2Crowd .docs-ng-view .doc-content {
        margin: 8px;
        padding: 0;
    }
    .G2Crowd .docs-ng-view docs-demo {
        padding: 0;
    }
    .G2Crowd .docs-ng-view docs-demo:first-child {
        margin-top: 0;
    }
    .G2Crowd .docs-ng-view docs-demo .doc-demo-content {
        margin: 0;
    }
}
