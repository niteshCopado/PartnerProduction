<aura:component>
    <aura:attribute name="headings" type="String[]" default="[]" />
    
    <!--<aura:attribute name="items" type="Object[]" />
    <aura:handler name="init" value="{! this }" action="{! c.doInit }" />
    <aura:handler name="change" value="{! v.items }" action="{! c.doInit }"/>-->
    
    <div>
        <div class="heading">
            <aura:iteration items="{! v.headings }" var="h">
                <div>{! h }</div>
            </aura:iteration> 
        </div>
       
        {! v.body }
    </div>
    
</aura:component>