                                                                                        <apex:selectOption itemLabel="--None--" itemValue="" />
                                                                                        <apex:selectOptions value="{!Orderinvoice_DateFields}"/>    
                                                                                    </apex:selectList>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                
                                                                <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-large-size_1-of-2">
                                                                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-large-size_1-of-2">
