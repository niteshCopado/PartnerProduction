<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <customPermissions>
        <enabled>true</enabled>
        <name>Monitor_Enable_Heroku_Settings</name>
    </customPermissions>
    <description>This permission sets gives users ability to see Heroku Settings page on the Monitor. This permission set does not grant access to Monitor itself.</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Copado Access Monitor Heroku Settings - No Action</label>
</PermissionSet>