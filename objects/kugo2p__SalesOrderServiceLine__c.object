    <validationRules>
        <fullName>kugo2p__ActiveServiceCheck</fullName>
        <active>true</active>
        <description>Ensure the service selected is active.</description>
        <errorConditionFormula>AND(kugo2p__Service__r.kugo2p__Active__c = false, ISNEW())</errorConditionFormula>
        <errorDisplayField>kugo2p__Service__c</errorDisplayField>
        <errorMessage>The selected Service is not active.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>kugo2p__Cant_Change_Service_Fields</fullName>
        <active>true</active>
        <description>Don&apos;t allow changes to the Service fields once saved.</description>
        <errorConditionFormula>ISCHANGED( kugo2p__Service__c )</errorConditionFormula>
        <errorDisplayField>kugo2p__Service__c</errorDisplayField>
        <errorMessage>Once a service line has been saved, the Service cannot be changed. Please create a new line and delete this one.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>kugo2p__ServiceCheck</fullName>
        <active>true</active>
        <errorConditionFormula>AND(kugo2p__Service__r.kugo2p__Service__c &lt;&gt; true, CreatedDate = LastModifiedDate )</errorConditionFormula>
        <errorDisplayField>kugo2p__Service__c</errorDisplayField>
        <errorMessage>Only Service Products Allowed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>kugo2p__ServicePercentofProductCheck</fullName>
        <active>true</active>
        <description>Prevents the user from saving the record if &quot;% of Unit Price&quot; and &quot;Order Product Line&quot; are not filled out together. If one is entered, both are required.</description>
        <errorConditionFormula>or(
	and(
		and(
			ISBLANK(kugo2p__OrderLinePercentofUnitPrice__c) = false,
			kugo2p__OrderLinePercentofUnitPrice__c &lt;&gt; 0.00,
			kugo2p__OrderLinePercentofUnitPrice__c &lt;&gt; 0
		),
		ISBLANK(kugo2p__SalesOrderProductLine__c) = true		
	),
	AND(
		OR(
			ISBLANK(kugo2p__OrderLinePercentofUnitPrice__c) = true,
			kugo2p__OrderLinePercentofUnitPrice__c = 0.00
		),
		ISBLANK(kugo2p__SalesOrderProductLine__c) = false		
	)
)</errorConditionFormula>
        <errorDisplayField>kugo2p__SalesOrderProductLine__c</errorDisplayField>
        <errorMessage>&quot;Order Line % of Unit Price&quot; and &quot;Order Product Line&quot; must be entered together.</errorMessage>
    </validationRules>
