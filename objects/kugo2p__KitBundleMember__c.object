    <validationRules>
        <fullName>kugo2p__InactiveMember</fullName>
        <active>true</active>
        <errorConditionFormula>kugo2p__ReferenceMemberProductService__r.kugo2p__Active__c = false</errorConditionFormula>
        <errorDisplayField>kugo2p__ReferenceMemberProductService__c</errorDisplayField>
        <errorMessage>Kit/Bundle Member Product is Not Active</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>kugo2p__KitandMemberCannotHaveSameName</fullName>
        <active>true</active>
        <errorConditionFormula>kugo2p__ReferenceMemberProductService__c = kugo2p__ReferenceKitBundle__c</errorConditionFormula>
        <errorDisplayField>kugo2p__ReferenceMemberProductService__c</errorDisplayField>
        <errorMessage>Kit/Bundle and Member Product Cannot Have the Same Name</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>kugo2p__MemberCannotBeKit</fullName>
        <active>true</active>
        <errorConditionFormula>kugo2p__ReferenceMemberProductService__r.kugo2p__KitBundle__c = true</errorConditionFormula>
        <errorDisplayField>kugo2p__ReferenceMemberProductService__c</errorDisplayField>
        <errorMessage>Member Cannot Be a Kit</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>kugo2p__MustBeKit</fullName>
        <active>true</active>
        <errorConditionFormula>kugo2p__ReferenceKitBundle__r.kugo2p__KitBundle__c = false &amp;&amp; TEXT(kugo2p__ReferenceKitBundle__r.kugo2p__ConfigurationMethod__c) != &apos;Kit/Bundle&apos;</errorConditionFormula>
        <errorDisplayField>kugo2p__ReferenceKitBundle__c</errorDisplayField>
        <errorMessage>Kit/Bundle Must Be Checked As A Kit</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>kugo2p__QuantityMustBeGreaterThanZero</fullName>
        <active>true</active>
        <errorConditionFormula>kugo2p__Quantity__c &lt;=0</errorConditionFormula>
        <errorDisplayField>kugo2p__ReferenceMemberProductService__c</errorDisplayField>
        <errorMessage>Kit/Bundle Member Must Have A Quantity Greater Than 0</errorMessage>
    </validationRules>
