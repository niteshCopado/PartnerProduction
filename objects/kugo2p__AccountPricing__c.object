    <fields>
        <fullName>kugo2p__IsActive__c</fullName>
        <deprecated>false</deprecated>
        <description>Active if Date End is Blank, or Today is within Date Start and Date End, or the Related Contract is Activated with Today is within Start Date and End Date.</description>
        <externalId>false</externalId>
        <formula>AND(
  OR(
    kugo2p__Type__c = &quot;Product/Service&quot;,
    kugo2p__Type__c = &quot;Filter&quot;
  ),
  OR(
    AND(
      ISBLANK(kugo2p__Contract__c),
      ISBLANK(kugo2p__DateStart__c),
      ISBLANK(kugo2p__DateEnd__c)
    ),
    AND(
      ISBLANK(kugo2p__Contract__c),
      kugo2p__DateStart__c &lt;= Today(),
      ISBLANK(kugo2p__DateEnd__c)
    ),
    AND(
      ISBLANK(kugo2p__Contract__c),
      ISBLANK(kugo2p__DateStart__c),
      kugo2p__DateEnd__c &gt;= Today()
    ),
    AND(
      ISBLANK(kugo2p__Contract__c),
      kugo2p__DateStart__c &lt;= Today(),
      kugo2p__DateEnd__c &gt;= Today()
    ),
    AND(
      TEXT(kugo2p__Contract__r.Status) = &apos;Activated&apos;,
      OR(
        AND(
          ISBLANK(kugo2p__Contract__r.StartDate),
          ISBLANK(kugo2p__Contract__r.EndDate)
        ),
        AND(
          kugo2p__Contract__r.StartDate &lt;= Today(),
          kugo2p__Contract__r.EndDate &gt;= Today()
        )
      )
    )
  )
)</formula>
        <inlineHelpText>Active if Date End is Blank, or Today is within Date Start and Date End, or the Related Contract is Activated with Today is within Start Date and End Date</inlineHelpText>
        <label>Active</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
