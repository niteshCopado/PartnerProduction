    <validationRules>
        <fullName>kugo2p__FilterCheck</fullName>
        <active>true</active>
        <description>If Filter function is used, Account Pricing requires Filter fields filled</description>
        <errorConditionFormula>AND(
  NOT(ISBLANK(kugo2p__FilterField__c)),
  OR(
    ISBLANK(TEXT(kugo2p__FilterObject__c)),
    ISBLANK(TEXT(kugo2p__Operator__c))
  )
)</errorConditionFormula>
        <errorMessage>You must enter Filter Object, Filter Field and Operator fields</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>kugo2p__PriceDiscountCheck</fullName>
        <active>true</active>
        <description>Account Pricing requires either Price or Discount</description>
        <errorConditionFormula>AND(
  kugo2p__Hide__c = FALSE,
  OR(
    AND(
      ISBLANK(kugo2p__Price__c),
      ISBLANK(kugo2p__Discount__c)
    ),
    AND(
      NOT(ISBLANK(kugo2p__Price__c)),
      NOT(ISBLANK(kugo2p__Discount__c))
    )
  )
)</errorConditionFormula>
        <errorMessage>You must enter either Price or Discount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>kugo2p__ProductFilterCheck</fullName>
        <active>true</active>
        <description>Account Pricing requires either Product/Service or Filter</description>
        <errorConditionFormula>AND(
  NOT(ISBLANK(kugo2p__ProductService__c)),
  OR(
    NOT(ISBLANK(TEXT(kugo2p__FilterObject__c))),
    NOT(ISBLANK(kugo2p__FilterField__c)),
    NOT(ISBLANK(TEXT(kugo2p__Operator__c)))
  )
)</errorConditionFormula>
        <errorMessage>You must enter either Product/Service or Filter Information</errorMessage>
    </validationRules>
