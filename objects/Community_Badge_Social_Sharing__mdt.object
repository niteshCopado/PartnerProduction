<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>This custom metadata stores configuration for certification badges which can be shared on social media like Linkedin, Twitter.</description>
    <fields>
        <fullName>Description__c</fullName>
        <description>Contains the description of certification badge.</description>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Description</label>
        <required>false</required>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>Redirect_URL__c</fullName>
        <description>Contains the redirect url for community badge.</description>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Redirect URL</label>
        <required>false</required>
        <type>Url</type>
    </fields>
    <fields>
        <fullName>Title__c</fullName>
        <description>Contains the title of certification badge.</description>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Title</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Twitter_Text__c</fullName>
        <description>Contains the default twitter text.</description>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Twitter Text</label>
        <required>false</required>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>Unique_Name__c</fullName>
        <description>Unique name of certification program.</description>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Unique Name</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Community Badge Social Sharing</label>
    <pluralLabel>Community Badge Social Sharings</pluralLabel>
    <visibility>Public</visibility>
</CustomObject>
