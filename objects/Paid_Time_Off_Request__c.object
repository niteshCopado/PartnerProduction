    <validationRules>
        <fullName>Negative_Number_of_Days_Worked_Holidays</fullName>
        <active>true</active>
        <description>This rule requires people to submit worked holidays as negative numbers.</description>
        <errorConditionFormula>AND(
    Working_Holiday__c,
    OR(
    ISBLANK(Number_of_days__c),
    NOT(Number_of_days__c=-1),
    (End_Date__c - Start_Date__c)=1
  )
)</errorConditionFormula>
        <errorMessage>Please ensure that each holiday worked is a single day (StartDate and EndDate must be the same day) and that the number is set to -1.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Negative_Number_of_Days_only_for_holiday</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  Working_Holiday__c=false,
  Number_of_days__c&lt;0
)</errorConditionFormula>
        <errorDisplayField>Number_of_days__c</errorDisplayField>
        <errorMessage>Number of Days must be greater than 0 if it is a holiday request.  Only requests for working on holidays, may have a negative number.</errorMessage>
    </validationRules>
