    <validationRules>
        <fullName>TopBoxWG__CSM_Tasks</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
				ISPICKVAL(TopBoxWG__Task_Type__c,&apos;CSM&apos;),
				OR(
				    ISBLANK(TEXT(TopBoxWG__CSM_Due_Weekday__c)),
				    ISBLANK(TEXT(TopBoxWG__CSM_Reminder_Weekday__c)), 
				    ISBLANK(TEXT(TopBoxWG__CSM_Task_Hour__c)),
  				  ISBLANK(TEXT(TopBoxWG__CSM_Task_Minute__c))
				)
)</errorConditionFormula>
        <errorMessage>All CSM fields are required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TopBoxWG__Feedback_Followup</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
				ISPICKVAL(TopBoxWG__Task_Type__c,&apos;Feedback Followup&apos;),
				ISBLANK(TEXT(TopBoxWG__Feedback_Followup_Type__c))
)</errorConditionFormula>
        <errorDisplayField>TopBoxWG__Feedback_Followup_Type__c</errorDisplayField>
        <errorMessage>Type is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TopBoxWG__NonCSM</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
				NOT(ISPICKVAL(TopBoxWG__Task_Type__c,&apos;CSM&apos;)),
				OR(
				    ISBLANK(TopBoxWG__Reminder_Hours__c),
				    ISBLANK(TopBoxWG__Due_Date_Hours__c)
				)
)</errorConditionFormula>
        <errorMessage>Due Date &amp; Reminder are required</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
