        <label>Business Number</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>amc__Credit_Name__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
