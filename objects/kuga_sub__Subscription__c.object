    <fields>
        <fullName>kuga_sub__Active__c</fullName>
        <deprecated>false</deprecated>
        <description>Field is Checked, if the following three conditions are met: 

(1) Associated Contract Record is Effective
(2) Associated Order Record Status is Approved or Released
(3) Subscription Dates are Blank or Today is within Start Date and End Date</description>
        <externalId>false</externalId>
        <formula>AND(
  kuga_sub__ContractNumber__r.kuga_sub__Effective__c = true,
  OR(
    kuga_sub__Order__r.kugo2p__RecordStatus__c == &apos;Approved&apos;,
    kuga_sub__Order__r.kugo2p__RecordStatus__c == &apos;Released&apos;
  ),
  OR(
    AND(
      ISBLANK(kuga_sub__StartDate__c),
      ISBLANK(kuga_sub__EndDate__c)
    ),
    AND(
      kuga_sub__StartDate__c &lt;= Today(),
      kuga_sub__EndDate__c &gt;= Today()
    )
  )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Active</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
