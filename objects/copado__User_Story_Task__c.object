    <fields>
        <fullName>Estimated_Time_Hours__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
    CONTAINS( Estimated_Time__c , &quot;d&quot;) &amp;&amp; CONTAINS( Estimated_Time__c , &quot;h&quot;),
    (VALUE(MID( SUBSTITUTE(Estimated_Time__c , &quot; &quot;, &quot;&quot;), 0, FIND(&quot;d&quot;, SUBSTITUTE(Estimated_Time__c , &quot; &quot;, &quot;&quot;))-1)) * 8 ) +
    (VALUE(MID( SUBSTITUTE(Estimated_Time__c , &quot; &quot;, &quot;&quot;), FIND(&quot;d&quot;, SUBSTITUTE(Estimated_Time__c , &quot; &quot;, &quot;&quot;))+1, (LEN(SUBSTITUTE(Estimated_Time__c , &quot; &quot;, &quot;&quot;)) - (FIND(&quot;d&quot;, SUBSTITUTE(Estimated_Time__c , &quot; &quot;, &quot;&quot;)) + 1) ) ))) ,
    IF(
        CONTAINS( Estimated_Time__c , &quot;h&quot;),
        (VALUE(MID( SUBSTITUTE(Estimated_Time__c , &quot; &quot;, &quot;&quot;), 0, FIND(&quot;h&quot;, SUBSTITUTE(Estimated_Time__c , &quot; &quot;, &quot;&quot;))-1)) ),
        IF(
            CONTAINS( Estimated_Time__c , &quot;d&quot;),
            (VALUE(MID( SUBSTITUTE(Estimated_Time__c , &quot; &quot;, &quot;&quot;), 0, FIND(&quot;d&quot;, SUBSTITUTE(Estimated_Time__c , &quot; &quot;, &quot;&quot;))-1)) * 8),
            0
        )
    )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
