    <fields>
        <fullName>kuga_sub__DiscountSalesPrice__c</fullName>
        <deprecated>false</deprecated>
        <description>Effective Price is the Sales Price with Discounts Applied.</description>
        <externalId>false</externalId>
        <formula>If (ISBLANK( kuga_sub__ServiceTerm__c ) || kuga_sub__ServiceTerm__c &lt;= 0,
round(UnitPrice * (1 - Discount), 2),
round(kuga_sub__NetAmount__c / Quantity / If(Text(kuga_sub__ServiceTermBehavior__c) = &quot;Excluded&quot;, 1, kuga_sub__ServiceTerm__c), 2)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Effective Price is the Sales Price with Discounts Applied</inlineHelpText>
        <label>Effective Price</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>kuga_sub__EffectiveDiscount__c</fullName>
        <deprecated>false</deprecated>
        <description>Effective Discount is based on Net Amount from List Amount.</description>
        <externalId>false</externalId>
        <formula>IF(
  kuga_sub__ListAmount__c = 0,
  NULL,
  IF(
    ListPrice == UnitPrice,
    Discount,
    (kuga_sub__ListAmount__c - kuga_sub__NetAmount__c) / kuga_sub__ListAmount__c
  )
)</formula>
        <inlineHelpText>Effective Discount is based on Net Amount from List Amount</inlineHelpText>
        <label>Effective Discount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Percent</type>
    </fields>
