        <externalId>false</externalId>
        <formula>IF( kuga_sub__ServiceTerm__c  = 0, &apos; &apos;, TEXT( kuga_sub__ServiceTerm__c ) + &apos; &apos; + TEXT ( kuga_sub__UnitofTerm__c )) + If( kuga_sub__ServiceTerm__c  &gt;1, &apos;s&apos;, &apos;&apos;)</formula>
