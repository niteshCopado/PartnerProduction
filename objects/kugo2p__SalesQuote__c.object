    <webLinks>
        <fullName>kugo2p__Delete</fullName>
        <availability>online</availability>
        <description>Mass Delete Quote Button</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Delete</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>//Include and initialize the AJAX Toolkit javascript library 
// 
{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/connection.js&quot;)} 

var idsToDelete={!GETRECORDIDS($ObjectType.kugo2p__SalesQuote__c)}; 
var deleteWarning=&apos;Are you sure you wish to delete &apos;+ idsToDelete.length+&apos; quotes?&apos;; 

if(idsToDelete.length &amp;&amp; (window.confirm(deleteWarning))) { 
	// Delete the records, and pass a function into the call 
	// so that the toolkit refreshes the current page 
	// asynchronously when the call succeeds. 

	sforce.connection.deleteIds(idsToDelete, function() { navigateToUrl(window.location.href); }); 
} else if (idsToDelete.length==0){ 
	alert(&quot;Please select the quotes you wish to delete.&quot;);
}</url>
    </webLinks>
    <webLinks>
        <fullName>kugo2p__DesignatePrimary</fullName>
        <availability>online</availability>
        <description>For Opportunity Page Layout only. Marks the selected Quote as Primary record.</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Designate Primary</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT (&quot;/soap/ajax/37.0/connection.js&quot;)}

var records = {!GETRECORDIDS($ObjectType.kugo2p__SalesQuote__c)};

if (records[0] == null) {

   alert(&quot;Please select a row to set Primary Quote.&quot;);

} else if (records.length &gt; 1) {
   alert(&quot;Please select only one Quote.&quot;);

} else {

    var newRecords = [];

    var item = new sforce.SObject(&quot;kugo2p__SalesQuote__c&quot;);
    item.id = records[0];
    item.kugo2p__IsPrimary__c = true;
    newRecords.push(item);

    result = sforce.connection.update(newRecords);

    window.location.reload();

}</url>
    </webLinks>
