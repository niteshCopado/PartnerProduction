    <validationRules>
        <fullName>TopBoxWG__Require_Task_Fields_if_using_Automation</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
				TopBoxWG__CSM_Task_Automation__c,
				ISBLANK(TopBoxWG__CSM_Task_Template__c)
)</errorConditionFormula>
        <errorDisplayField>TopBoxWG__CSM_Task_Template__c</errorDisplayField>
        <errorMessage>A template must be selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TopBoxWG__Task_Send_Types</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
					TopBoxWG__CSM_Task_Automation__c == TRUE,
					TopBoxWG__CSM_Task_Onboarding__c == FALSE,
					TopBoxWG__CSM_Task_Initial__c == FALSE,
					TopBoxWG__CSM_Task_Renewal__c == FALSE,
					TopBoxWG__CSM_Task_Ongoing__c == FALSE
	)</errorConditionFormula>
        <errorDisplayField>TopBoxWG__CSM_Task_Automation__c</errorDisplayField>
        <errorMessage>You must check at least one of the send types when using Task Automation</errorMessage>
    </validationRules>
