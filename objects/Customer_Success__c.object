        <fullName>Account_Health_Red</fullName>
        <active>true</active>
        <description>Account Health Code Summary required if the Account Health is Red.</description>
        <errorConditionFormula>AND(
  ISBLANK(Account_Health_Code_Summary__c ),
  ISPICKVAL(Account_Health__c, &quot;Red&quot;)
)</errorConditionFormula>
        <errorMessage>Account Health Code Summary is required when the Health Code is Red.</errorMessage>
