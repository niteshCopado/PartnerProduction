    <fields>
        <fullName>kugo2p__TrackThisShipment__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF( kugo2p__Carrier__r.Name == &quot;USPS&quot;, HYPERLINK(&apos;http://trkcnfrm1.smi.usps.com/PTSInternetWeb/InterLabelInquiry.do?origTrackNum=&apos;&amp;kugo2p__TrackingNumber__c, &quot;Track with USPS&quot;, &quot;_blank&quot;), 

IF(kugo2p__Carrier__r.Name == &quot;UPS&quot;,HYPERLINK(&apos;http://wwwapps.ups.com/WebTracking/track?track=yes&amp;trackNums=&apos;&amp;kugo2p__TrackingNumber__c,&quot;Track with UPS&quot;,&quot;_blank&quot;), 

IF(kugo2p__Carrier__r.Name == &quot;FedEx&quot;,HYPERLINK(&apos;http://www.fedex.com/Tracking?action=track&amp;tracknumbers=&apos;&amp;kugo2p__TrackingNumber__c,&quot;Track with FedEx&quot;,&quot;_blank&quot;), 

IF(kugo2p__Carrier__r.Name == &quot;DHL&quot;, HYPERLINK(&apos;http://www.dhl.com/content/g0/en/express/tracking.shtml?brand=DHL&amp;AWB=&apos;&amp;kugo2p__TrackingNumber__c, &quot;Track with DHL&quot;, &quot;_blank&quot;),&quot;&quot;) 
) 
) 
)</formula>
        <label>Track this Shipment</label>
