    <webLinks>
        <fullName>kugo2p__Set_Default_Method</fullName>
        <availability>online</availability>
        <description>Marks the selected Payment Method the default record.</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Set Default</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT (&quot;/soap/ajax/16.0/connection.js&quot;)} 
{!REQUIRESCRIPT (&quot;/soap/ajax/16.0/apex.js&quot;)}

var records = {!GETRECORDIDS($ObjectType.kugo2p__Payment_Method__c)}; 
var newRecords = [];
var contactId;
if (records[0] == null || records.length &gt;1) {

    if (records[0] == null) alert(&quot;Please select a row to mark default&quot;) ;
    if (records.length &gt;1) alert(&quot;Please select only one row&quot;) ;

} else { 

for (var n in records) {

// query in the for loop is fine, as the users are allowed to select only one record in the list
var query = &quot;SELECT Id,  kugo2p__Expiration_Year__c, kugo2p__Expiration_Month__c,kugo2p__IsActive__c FROM kugo2p__Payment_Method__c WHERE Id= &apos;&quot; + records[0] + &quot;&apos;&quot;;
var resultRecords = sforce.connection.query(query).getArray(&quot;records&quot;);

var expStatus = resultRecords[0].kugo2p__IsActive__c;

if (expStatus.indexOf(&quot;red&quot;) != -1) {
    alert(&apos;Default Payment Method must be Active.&apos;);
    break;
}

var item = new sforce.SObject(&quot;kugo2p__Payment_Method__c&quot;);
item.id = records[n];
item.kugo2p__Default__c = true;
newRecords.push(item);
break; // only process one record if more than one is selected

} 

if (expStatus.indexOf(&quot;red&quot;) == -1) {
    result = sforce.connection.update(newRecords); 
    window.location.reload();
}

}</url>
    </webLinks>
