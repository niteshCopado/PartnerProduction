        <fullName>kugo2p__IsActive__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(OR( AND(TEXT(kugo2p__Type__c) == &apos;Paypal&apos;, ISBLANK(kugo2p__Expiration_Year__c), ISPICKVAL(kugo2p__Expiration_Month__c, &apos;&apos;)), CONTAINS(Name, &apos;Bank Account&apos;)), IMAGE(&quot;/img/samples/flag_green.gif&quot;,&quot;green&quot;),

IF(  
OR(VALUE(kugo2p__Expiration_Year__c)  &lt; YEAR(TODAY()), ISBLANK(kugo2p__Expiration_Year__c)
, ISPICKVAL(kugo2p__Expiration_Month__c, &apos;&apos;) , ISPICKVAL( kugo2p__Card_Type__c , &apos;&apos;) 
), IMAGE(&quot;/img/samples/flag_red.gif&quot;,&quot;red&quot;),  

IF( AND( VALUE(TEXT(kugo2p__Expiration_Month__c))  &lt; MONTH( TODAY() ), VALUE ( kugo2p__Expiration_Year__c )   = YEAR( TODAY() )) , IMAGE(&quot;/img/samples/flag_red.gif&quot;,&quot;red&quot;), 

IMAGE(&quot;/img/samples/flag_green.gif&quot;,&quot;green&quot;))))</formula>
        <inlineHelpText>Green = Active
Red = Not Active</inlineHelpText>
