    <validationRules>
        <fullName>Time_Spent_must_be_at_least_1_hour</fullName>
        <active>true</active>
        <description>This rule prevents from logging 0 or negative time.</description>
        <errorConditionFormula>Time_Spent_hours__c  &lt; 1</errorConditionFormula>
        <errorDisplayField>Time_Spent_hours__c</errorDisplayField>
        <errorMessage>Time Spent must be at least 1 hour.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Work_Log_Single_Lookup</fullName>
        <active>true</active>
        <description>This rule checks if the work log has only User Story or User Story Task assigned.</description>
        <errorConditionFormula>AND( NOT( ISBLANK( User_Story_Task__c ) ) ,  NOT( ISBLANK( User_Story__c ) ) )</errorConditionFormula>
        <errorMessage>You cannot have a Work Log assigned to both User Story and a User Story Task at the same time. Time spent will be automatically rolled up from User Story Tasks and the User Story itself.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
