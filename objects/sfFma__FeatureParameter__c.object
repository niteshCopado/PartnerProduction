    <validationRules>
        <fullName>sfFma__DataFlowDirectionUpdatePrevention</fullName>
        <active>true</active>
        <description>Data Flow Direction can only be updated by the Feature Management App.</description>
        <errorConditionFormula>AND(  ISCHANGED( sfFma__DataFlowDirection__c ),
 $User.Id &lt;&gt;  CreatedById 
)</errorConditionFormula>
        <errorDisplayField>sfFma__DataFlowDirection__c</errorDisplayField>
        <errorMessage>This value is set by the installed package and cannot be changed. Click undo or cancel to continue.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sfFma__DataTypeUpdatePrevention</fullName>
        <active>true</active>
        <description>Data Type can only be created or updated by the Feature Management App.</description>
        <errorConditionFormula>AND( ISCHANGED( sfFma__DataType__c ),
 $User.Id &lt;&gt;  CreatedById )</errorConditionFormula>
        <errorDisplayField>sfFma__DataType__c</errorDisplayField>
        <errorMessage>This value is set by the installed package and cannot be changed. Click undo or cancel to continue.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sfFma__FullNameUpdatePrevention</fullName>
        <active>true</active>
        <description>Feature Parameter Full Name can only be updated by the Feature Management App.</description>
        <errorConditionFormula>AND( ISCHANGED( sfFma__FullName__c ),
 $User.Id &lt;&gt;  CreatedById )</errorConditionFormula>
        <errorDisplayField>sfFma__FullName__c</errorDisplayField>
        <errorMessage>This value is set by the installed package and cannot be changed. Click undo or cancel to continue.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sfFma__XSS_Prevention_FullName</fullName>
        <active>true</active>
        <description>Prevent malicious code from being introduced into our text fields.</description>
        <errorConditionFormula>NOT(REGEX(sfFma__FullName__c , &quot;\\w+__\\w+&quot;))</errorConditionFormula>
        <errorDisplayField>sfFma__FullName__c</errorDisplayField>
        <errorMessage>Full Name must be in the format of &lt;namespace&gt;__&lt;developer name&gt;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sfFma__XSS_Prevention_Namespace</fullName>
        <active>true</active>
        <description>Prevent malicious code from being stored in the Namespace Prefix text field.</description>
        <errorConditionFormula>NOT(REGEX(sfFma__Namespace_Prefix__c , &quot;\\w+&quot;))</errorConditionFormula>
        <errorDisplayField>sfFma__Namespace_Prefix__c</errorDisplayField>
        <errorMessage>Namespace prefix must only contain alpha numeric characters and/or underscores.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sfFma__XSS_Prevention_Version</fullName>
        <active>true</active>
        <description>Prevent malicious code from being stored in the Introduced in Package Version field.</description>
        <errorConditionFormula>NOT(REGEX(sfFma__IntroducedInPackageVersion__c, &quot;^(\\w{15,18}|\\d+\\.\\d+)$&quot;))</errorConditionFormula>
        <errorDisplayField>sfFma__IntroducedInPackageVersion__c</errorDisplayField>
        <errorMessage>Version number strings must be in the format of &lt;major version&gt;.&lt;minor version&gt;</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
