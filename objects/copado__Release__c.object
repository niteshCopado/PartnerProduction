    <validationRules>
        <fullName>Provide_a_blocking_reason</fullName>
        <active>true</active>
        <description>If the release is blocked a reason must be provided</description>
        <errorConditionFormula>AND(
TEXT(copado__Status__c)==&quot;Blocked&quot;,
ISBLANK(Blocking_reason__c)
)</errorConditionFormula>
        <errorDisplayField>Blocking_reason__c</errorDisplayField>
        <errorMessage>You must provide a blocking reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>copado__Validate_spaces_in_version</fullName>
        <active>true</active>
        <description>Make sure no spaces are there in version field</description>
        <errorConditionFormula>CONTAINS(copado__Version__c,&quot; &quot;)</errorConditionFormula>
        <errorDisplayField>copado__Version__c</errorDisplayField>
        <errorMessage>please dont include spaces in version field</errorMessage>
    </validationRules>
