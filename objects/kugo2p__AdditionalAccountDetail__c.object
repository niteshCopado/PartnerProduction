    <fields>
        <fullName>kugo2p__BillingFrequency__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(
  TEXT(kugo2p__InvoiceSchedule__r.kugo2p__SchedulePeriod__c) == &apos;Daily&apos;,
  &apos;Daily&apos;,
  IF(
    TEXT(kugo2p__InvoiceSchedule__r.kugo2p__SchedulePeriod__c) == &apos;Weekly&apos;,
    &apos;Weekly&apos;,
    IF(
      TEXT(kugo2p__InvoiceSchedule__r.kugo2p__SchedulePeriod__c) == &apos;Monthly&apos;,
      &apos;Monthly&apos;,
      IF(
        TEXT(kugo2p__InvoiceSchedule__r.kugo2p__SchedulePeriod__c) == &apos;Quarterly&apos;,
        &apos;Quarterly&apos;,
        IF(
          TEXT(kugo2p__InvoiceSchedule__r.kugo2p__SchedulePeriod__c) == &apos;Semi-Annually&apos;,
          &apos;Semi-Annually&apos;,
          IF(
            TEXT(kugo2p__InvoiceSchedule__r.kugo2p__SchedulePeriod__c) == &apos;Annually&apos;,
            &apos;Annually&apos;,
            IF(
              NOT(ISBLANK(TEXT(kugo2p__InvoiceSchedule__r.kugo2p__SchedulePeriod__c))),
              TEXT(kugo2p__InvoiceSchedule__r.kugo2p__SchedulePeriod__c),
              &apos;One-Time&apos;
            )
          )
        )
      )
    )
  )
)</formula>
        <label>Billing Frequency</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
