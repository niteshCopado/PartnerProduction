    <validationRules>
        <fullName>kugo2p__DiscountCheck</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  TEXT(kugo2p__PriceBehavior__c) = &quot;Discounted&quot;,
  OR(
    AND(
      ISBLANK(kugo2p__LineDiscount__c),
      ISBLANK(kugo2p__PriceDiscount__c)
    ),
    AND(
      NOT(ISBLANK(kugo2p__LineDiscount__c)),
      NOT(ISBLANK(kugo2p__PriceDiscount__c))
    )
  )
)</errorConditionFormula>
        <errorMessage>You must enter either Line Discount or Price Discount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>kugo2p__MaximumQuantityDefaultCheck</fullName>
        <active>true</active>
        <errorConditionFormula>kugo2p__MaximumQuantity__c &lt; kugo2p__DefaultQuantity__c</errorConditionFormula>
        <errorDisplayField>kugo2p__MaximumQuantity__c</errorDisplayField>
        <errorMessage>Maximum Quantity must be greater than or equal to Default Quantity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>kugo2p__MaximumQuantityMinimumCheck</fullName>
        <active>true</active>
        <errorConditionFormula>kugo2p__MaximumQuantity__c &lt; kugo2p__MinimumQuantity__c</errorConditionFormula>
        <errorDisplayField>kugo2p__MaximumQuantity__c</errorDisplayField>
        <errorMessage>Maximum Quantity must be greater than or equal to Minimum Quantity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>kugo2p__MaximumQuantityZeroCheck</fullName>
        <active>true</active>
        <errorConditionFormula>kugo2p__MaximumQuantity__c &lt;= 0</errorConditionFormula>
        <errorDisplayField>kugo2p__MaximumQuantity__c</errorDisplayField>
        <errorMessage>Maximum Quantity must be greater than Zero</errorMessage>
    </validationRules>
