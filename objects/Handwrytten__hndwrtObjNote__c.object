 ISPICKVAL( Handwrytten__Status__c , &apos;In-Progress&apos;)
 ),
 NOT (ISBLANK(Handwrytten__gift_card_id__c) ),
 Handwrytten__gift_card_id__c &lt;&gt; &apos;000&apos;,
 ISBLANK( Handwrytten__denomination_id__c )
)</errorConditionFormula>
