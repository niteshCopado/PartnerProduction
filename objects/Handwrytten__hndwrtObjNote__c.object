        <errorConditionFormula>AND ( 
ISPICKVAL( Handwrytten__Status__c , &apos;Completed&apos;), 
ISBLANK( Handwrytten__credit_card_id__c ),
BLANKVALUE( Handwrytten__invoiced__c, &apos;0&apos;) = &apos;0&apos;  
)</errorConditionFormula>
        <errorMessage>Credit Card is required to complete order. Please select a credit card.</errorMessage>
