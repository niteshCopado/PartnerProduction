        <description>Users must select a Handwrytten Card to complete order</description>
        <errorConditionFormula>AND (
 OR (
 ISPICKVAL( Handwrytten__Status__c , &apos;Completed&apos;),
 ISPICKVAL( Handwrytten__Status__c , &apos;In-Progress&apos;)
 ),
 ISBLANK( Handwrytten__card_id__c )
)</errorConditionFormula>
