    <fields>
        <fullName>Expiration_Date__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Value will be set to 0 when expired</inlineHelpText>
        <label>Expiration Date</label>
        <required>false</required>
        <trackHistory>true</trackHistory>
        <trackTrending>true</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>External_Id__c</fullName>
        <caseSensitive>false</caseSensitive>
        <externalId>true</externalId>
        <label>External Id</label>
        <length>255</length>
        <required>false</required>
        <trackHistory>true</trackHistory>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>Is_Checked__c</fullName>
        <externalId>false</externalId>
        <formula>IF(TODAY()+7 =  Expiration_Date__c , true, false)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Is checked true when expiration date is expire before 7 days.</inlineHelpText>
        <label>Is Checked</label>
        <trackHistory>false</trackHistory>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Salesforce_Org__c</fullName>
        <externalId>false</externalId>
