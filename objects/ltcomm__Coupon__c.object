    <validationRules>
        <fullName>ltcomm__Disable_Negative_Integers</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
            ltcomm__Min_Seats__c &lt; 0, ltcomm__Max_Discount__c &lt; 0,
            ltcomm__Min_Cart__c &lt; 0, ltcomm__Max_Lifetime_Discount__c &lt; 0,
            ltcomm__Quantity__c &lt; 0
            )</errorConditionFormula>
        <errorMessage>The integer must be positive</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ltcomm__Percentage_Coupon_Value</fullName>
        <active>true</active>
        <description>If a user selects a coupon type = percentage, the &quot;Value&quot; field should be limited from 0
            to 100.</description>
        <errorConditionFormula>AND(ISPICKVAL(ltcomm__Type__c , &apos;Percentage&apos;),
            OR(ltcomm__Value__c &lt; 0, ltcomm__Value__c &gt; 100))</errorConditionFormula>
        <errorDisplayField>ltcomm__Value__c</errorDisplayField>
        <errorMessage>Value must be between 0 and 100.</errorMessage>
    </validationRules>
