        <fullName>amc__Tax_Amount_Outstanding__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>If(  amc__Tax_Amount__c  &gt; 0 ,amc__Total_Outstanding__c - (amc__Total_Outstanding__c / (1 + amc__Tax_Rate__c)), 0)</formula>
