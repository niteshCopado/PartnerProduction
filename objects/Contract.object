        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT (&quot;/soap/ajax/16.0/connection.js&quot;)}
{!REQUIRESCRIPT (&quot;/soap/ajax/16.0/apex.js&quot;)}

var records = {!GETRECORDIDS($ObjectType.Contract)};

if (records[0] == null || records.length &gt; 1) {

    if (records[0] == null) alert(&quot;Please select a Contract.&quot;) ;
    if (records.length &gt;1) alert(&quot;Please select only one Contract.&quot;) ;

} else {

    // query the Contract status
    var query = &quot;SELECT Id, Status FROM Contract WHERE Id= &apos;&quot; + records[0] + &quot;&apos;&quot;;
    var contract = sforce.connection.query(query).getArray(&quot;records&quot;)[0];

    var status = contract.Status;
    if (status == &apos;Cancelled&apos;) {
        alert(&apos;You cannot create an Expansion Opportunity from a Cancelled Contract.&apos;);
    } else {
        window.top.location.href = &apos;/apex/kuga_sub__kontroller?action=newExpansionOpportunity&amp;id=&apos; + records[0];
    }
}</url>
    </webLinks>
