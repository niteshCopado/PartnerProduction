    <validationRules>
        <fullName>kugo2p__Payment_Terms_Due_Date_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( kugo2p__PaymentTerms__c , &quot;Due Upon Receipt&quot;), OR(kugo2p__DaysTillPaymentDue__c  &lt;&gt; 0, ISBLANK(kugo2p__DaysTillPaymentDue__c)))</errorConditionFormula>
        <errorMessage>The Days Till Payment Due Date must equal &quot;0&quot; zero days if the Payment Term is Due Upon Receipt.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>kugo2p__Payment_Terms_Due_Date_Second_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( kugo2p__PaymentTerms__c , &quot;Due Upon Receipt&quot;)= FALSE, ISBLANK(kugo2p__DaysTillPaymentDue__c))</errorConditionFormula>
        <errorMessage>Please enter a number of days to the Days Till Payment Due Date field that matches the Payment Term.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>kugo2p__VAT_check</fullName>
        <active>true</active>
        <errorConditionFormula>or(
and(
kugo2p__TaxableVAT__c = false, 
ISBLANK(kugo2p__VAT__c) = false	
),
AND(
kugo2p__TaxableVAT__c  = true, 
ISBLANK( kugo2p__VAT__c) = true	
)
)</errorConditionFormula>
        <errorMessage>A valid default VAT setting must have both a Listing for &quot;VAT&quot; and a Check for &quot;Taxable VAT&quot;</errorMessage>
    </validationRules>
