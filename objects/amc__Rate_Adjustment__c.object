    <validationRules>
        <fullName>amc__Ensure_Correct_Fields_on_Role_Override</fullName>
        <active>true</active>
        <description>Ensures Role and Project are not Blank if Rate Type = Role-based Override</description>
        <errorConditionFormula>AND(
 ISPICKVAL(amc__Rate_Type__c, &quot;Role-based Override&quot;), 
 OR(
  ISBLANK(amc__Project__c), 
  ISBLANK(amc__Role__c), 
  NOT(ISBLANK(amc__Skill__c))
))</errorConditionFormula>
        <errorMessage>Project &amp; Role must contain a value, Skill must be empty</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>amc__Ensure_Correct_Fields_on_Role_Rate</fullName>
        <active>true</active>
        <description>Ensures Role is not Blank if Rate Type = Role-based</description>
        <errorConditionFormula>AND(
 ISPICKVAL(amc__Rate_Type__c, &quot;Role-based&quot;), 
 OR(
  ISBLANK(amc__Role__c), 
  NOT(ISBLANK(amc__Project__c)), 
  NOT(ISBLANK(amc__Skill__c))
 )
)</errorConditionFormula>
        <errorMessage>Role must contain a value, Project and Skill must be empty</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>amc__Ensure_Correct_Fields_on_Skill_Override</fullName>
        <active>true</active>
        <description>Ensures Skill, Role and Project are not Blank if Rate Type = Skill-based Override</description>
        <errorConditionFormula>AND(
 ISPICKVAL(amc__Rate_Type__c, &quot;Skill-based Override&quot;), 
 OR(
  ISBLANK(amc__Project__c), 
  ISBLANK(amc__Skill__c), 
  ISBLANK(amc__Role__c))
)</errorConditionFormula>
        <errorMessage>Skill, Project &amp; Role must contain a value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>amc__Ensure_Correct_Fields_on_Skill_Rate</fullName>
        <active>true</active>
        <description>Ensures Skill is not Blank if Rate Type = Skill-based</description>
        <errorConditionFormula>AND(
 ISPICKVAL(amc__Rate_Type__c, &quot;Skill-based&quot;), 
 OR(
  ISBLANK(amc__Skill__c), 
  ISBLANK(amc__Role__c),
  NOT(ISBLANK(amc__Project__c))
 )
)</errorConditionFormula>
        <errorMessage>Skill and Role must contain a value, Project must be empty.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>amc__Ensure_Correct_Fields_on_Standard_Rate</fullName>
        <active>true</active>
        <description>Ensures Role and Skill are Blank if Project Rate Type is Standard</description>
        <errorConditionFormula>AND(
 ISPICKVAL(amc__Rate_Type__c, &quot;Standard&quot;), 
 OR(
  ISBLANK(amc__Project__c), 
  NOT(ISBLANK(amc__Role__c)), 
  NOT(ISBLANK(amc__Skill__c))
 )
)</errorConditionFormula>
        <errorMessage>Project must contain a value, Role and Skill must be empty</errorMessage>
    </validationRules>
