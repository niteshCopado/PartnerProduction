        <fullName>kugo2p__Status__c</fullName>
        <deprecated>false</deprecated>
        <description>This field displays the status of the Order with the relevant actions, if applicable, that can be performed:

- Draft, Sent = Edits Allowed, No Payments.

- Approved = Edits Allowed, Accept Payments, Create Assets

- Released = No Edits, Accept Payments, Creates Shipments and Invoices.

- Cancelled = No Payments, Account Balance Not Updated.</description>
        <externalId>false</externalId>
        <formula>IF(
  kugo2p__RecordStatus__c = &apos;Draft&apos;,
  kugo2p__RecordStatus__c &amp; &quot; [&quot; &amp; HYPERLINK(&apos;/apex/kugo2p__kontroller?action=updateOrderStatus&amp;status=Sent&amp;ordId=&apos; &amp; Id, &apos;Submit&apos;, &quot;_self&quot;) &amp; &quot;] &quot; &amp; &quot; [&quot; &amp; HYPERLINK(&apos;/apex/kugo2p__kontroller?action=updateOrderStatus&amp;status=Approved&amp;ordId=&apos; &amp; Id, &apos;Approve&apos;, &quot;_self&quot;) &amp; &quot;] &quot; &amp; &quot; [&quot; &amp; HYPERLINK(&apos;/apex/kugo2p__kontroller?action=updateOrderStatus&amp;status=Released&amp;ordId=&apos; &amp; Id, &apos;Release&apos;, &quot;_self&quot;) &amp; &quot;] &quot; &amp; &quot; [&quot; &amp; HYPERLINK(&apos;/apex/kugo2p__kontroller?action=updateOrderStatus&amp;status=Cancelled&amp;ordId=&apos; &amp; Id, &apos;Cancel&apos;, &quot;_self&quot;) &amp; &quot;] &quot;,
  IF(
    kugo2p__RecordStatus__c = &apos;Sent&apos;,
    kugo2p__RecordStatus__c &amp; &quot; [&quot; &amp; HYPERLINK(&apos;/apex/kugo2p__kontroller?action=updateOrderStatus&amp;status=Approved&amp;ordId=&apos; &amp; Id, &apos;Approve&apos;, &quot;_self&quot;) &amp; &quot;] &quot; &amp; &quot; [&quot; &amp; HYPERLINK(&apos;/apex/kugo2p__kontroller?action=updateOrderStatus&amp;status=Released&amp;ordId=&apos; &amp; Id, &apos;Release&apos;, &quot;_self&quot;) &amp; &quot;] &quot; &amp; &quot; [&quot; &amp; HYPERLINK(&apos;/apex/kugo2p__kontroller?action=updateOrderStatus&amp;status=Cancelled&amp;ordId=&apos; &amp; Id, &apos;Cancel&apos;, &quot;_self&quot;) &amp; &quot;] &quot;,
    IF(
    kugo2p__RecordStatus__c = &apos;Approved&apos;,
      kugo2p__RecordStatus__c &amp; &quot; [&quot; &amp; HYPERLINK(&apos;/apex/kugo2p__kontroller?action=updateOrderStatus&amp;status=Released&amp;ordId=&apos; &amp; Id, &apos;Release&apos;, &quot;_self&quot;) &amp; &quot;] &quot; &amp; &quot; [&quot; &amp; HYPERLINK(&apos;/apex/kugo2p__kontroller?action=updateOrderStatus&amp;status=CancelApproved&amp;ordId=&apos; &amp; Id, &apos;Cancel&apos;, &quot;_self&quot;) &amp; &quot;] &quot;,
      kugo2p__RecordStatus__c &amp; 
      IF(
        kugo2p__RecordStatus__c = &apos;Released&apos;,
        &quot; [&quot; &amp; HYPERLINK(&apos;/apex/kugo2p__kontroller?action=updateOrderStatus&amp;status=Unrelease&amp;ordId=&apos; &amp; Id, &apos;Unrelease&apos;, &quot;_self&quot;) &amp; &quot;] &quot; &amp; &quot; [&quot; &amp; HYPERLINK(&apos;/apex/kugo2p__kontroller?action=updateOrderStatus&amp;status=CancelReleased&amp;ordId=&apos; &amp; Id, &apos;Cancel&apos;, &quot;_self&quot;) &amp; &quot;] &quot;,
