    <fields>
        <fullName>kugo2p__DiscountSalesPrice__c</fullName>
        <deprecated>false</deprecated>
        <description>Effective Price is the Sales Price with Discounts Applied.</description>
        <externalId>false</externalId>
        <formula>If(
  kugo2p__Quantity__c = 0,
  round(kugo2p__SalesPrice__c * (1 - (kugo2p__QuoteDiscountPercent__c + kugo2p__LineDiscountPercent__c)), 2),
  round(kugo2p__TotalAmount__c / kugo2p__Quantity__c, 2)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Effective Price is the Sales Price with Discounts Applied</inlineHelpText>
        <label>Effective Price</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>kugo2p__EffectiveDiscount__c</fullName>
        <deprecated>false</deprecated>
        <description>Effective Discount is based on Total Amount from List Amount.</description>
        <externalId>false</externalId>
        <formula>IF(
  kugo2p__ListAmount__c = 0,
  NULL,
  IF(
    kugo2p__ListPrice__c == kugo2p__SalesPrice__c,
    (kugo2p__QuoteDiscountPercent__c + kugo2p__LineDiscountPercent__c),
    (kugo2p__ListAmount__c - kugo2p__TotalAmount__c) / kugo2p__ListAmount__c
  )
)</formula>
        <inlineHelpText>Effective Discount is based on Total Amount from List Amount</inlineHelpText>
        <label>Effective Discount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
