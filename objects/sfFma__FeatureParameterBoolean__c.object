    <validationRules>
        <fullName>sfFma__FullNameUpdatePrevention</fullName>
        <active>true</active>
        <description>Full Name can only be updated by the Feature Management App.</description>
        <errorConditionFormula>AND(ISCHANGED( sfFma__FullName__c ),
 $User.Id &lt;&gt;  CreatedById )</errorConditionFormula>
        <errorDisplayField>sfFma__FullName__c</errorDisplayField>
        <errorMessage>This value is set by the installed package and cannot be changed. Click undo or cancel to continue.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sfFma__MetricValueUpdatePrevention</fullName>
        <active>true</active>
        <description>&quot;Subscriber To LMO&quot; Feature Parameter Values cannot be manually created or updated.</description>
        <errorConditionFormula>AND(
AND( ISPICKVAL( sfFma__FeatureParameter__r.sfFma__DataFlowDirection__c , &quot;Subscriber To LMO&quot;),
  $User.Id &lt;&gt;  sfFma__FeatureParameter__r.CreatedById ),
 ISCHANGED( sfFma__Value__c ) )</errorConditionFormula>
        <errorDisplayField>sfFma__Value__c</errorDisplayField>
        <errorMessage>Subscriber To LMO feature parameter values can only be created or updated by the Feature Management app. Click undo or cancel to continue.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>sfFma__XSS_Prevention_FullName</fullName>
        <active>true</active>
        <description>Prevent malicious code from being introduced into our text fields.</description>
        <errorConditionFormula>AND(NOT(REGEX(sfFma__FullName__c , &quot;\\w+__\\w+&quot;)),
NOT(ISBLANK( sfFma__FullName__c )))</errorConditionFormula>
        <errorDisplayField>sfFma__FullName__c</errorDisplayField>
        <errorMessage>Full Name must be in the format of &lt;namespace&gt;__&lt;developer name&gt;</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
