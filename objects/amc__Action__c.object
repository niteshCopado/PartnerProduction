    <validationRules>
        <fullName>amc__Billable_Hours_Cannot_Be_Entered</fullName>
        <active>false</active>
        <description>If the non-billable indicator on the action is True, then the Hours Scheduled - Billable cannot be greater than Zero.</description>
        <errorConditionFormula>amc__Non_Billable_Indicator__c = TRUE
 &amp;&amp; amc__Hours_Scheduled__c &gt; 0</errorConditionFormula>
        <errorDisplayField>amc__Hours_Scheduled__c</errorDisplayField>
        <errorMessage>You cannot enter billable hours for this action as the non-billable indicator is set to True.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>amc__End_Date_Exceeds_Milestone_Deadline</fullName>
        <active>false</active>
        <description>Ensure the Action&apos;s End Date does not exceed the Milestone&apos;s Milestone Deadline</description>
        <errorConditionFormula>amc__End_Date__c &gt;  amc__Milestone__r.amc__Milestone_Deadline__c</errorConditionFormula>
        <errorMessage>The Action&apos;s End Date must not exceed the Milestone Deadline</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>amc__End_Date_Validation</fullName>
        <active>true</active>
        <description>The end date must be greater or equal to the start date</description>
        <errorConditionFormula>amc__Start_Date__c &gt;  amc__End_Date__c</errorConditionFormula>
        <errorDisplayField>amc__End_Date__c</errorDisplayField>
        <errorMessage>The End Date must be greater or equal to the Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>amc__Exceed_Milestone_Budget</fullName>
        <active>false</active>
        <description>Ensures the Action&apos;s &apos;Hours Scheduled Value (Billable) does not exceed the Milestone&apos;s Unallocated Milestone Budget - applies only for Fixed Price Projects.</description>
        <errorConditionFormula>AND( ISPICKVAL( amc__Milestone__r.amc__Project__r.amc__Billing_Type__c , &quot;Fixed Price&quot;) , 

IF(ISCHANGED(amc__Hours_Scheduled_Cost__c), (amc__Milestone__r.amc__Hours_Scheduled_Cost__c - PRIORVALUE(amc__Hours_Scheduled_Cost__c)) + amc__Hours_Scheduled_Cost__c, amc__Milestone__r.amc__Hours_Scheduled_Cost__c + amc__Hours_Scheduled_Cost__c) 

&gt; amc__Milestone__r.amc__Milestone_Budget__c )</errorConditionFormula>
        <errorMessage>This Action will exceed the &apos;Milestone Budget&apos;, please reduce the Hours Scheduled (Billable)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>amc__Recurring_Validation_Frequency</fullName>
        <active>true</active>
        <errorConditionFormula>AND(amc__Is_Recurring__c, ISBLANK(TEXT(amc__Recurring_Frequency__c)))</errorConditionFormula>
        <errorDisplayField>amc__Recurring_Frequency__c</errorDisplayField>
        <errorMessage>You must set the Recurring Frequency</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>amc__Recurring_Validation_Number</fullName>
        <active>true</active>
        <errorConditionFormula>AND(amc__Is_Recurring__c, ISBLANK(amc__Number_of_Recurrences__c))</errorConditionFormula>
        <errorDisplayField>amc__Number_of_Recurrences__c</errorDisplayField>
        <errorMessage>You must set the Number of Recurrences</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>amc__Related_Task_Validation</fullName>
        <active>false</active>
        <description>The related task must have an end date, less or equal to the start date of the  child task</description>
        <errorConditionFormula>amc__Dependant_Action__r.amc__End_Date__c &gt;= amc__Start_Date__c</errorConditionFormula>
        <errorDisplayField>amc__Start_Date__c</errorDisplayField>
        <errorMessage>The dependant action has an end date greater than the start date of the current action.  Please adjust your dates accordingly.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>amc__Skill_Required</fullName>
        <active>true</active>
        <description>ensures the ‘Skill’ field is populated on the Action if the Project ‘Billing Type’ or ‘Cost Type’ are set to ‘Skill-based’</description>
        <errorConditionFormula>AND(ISBLANK( amc__Skill__c ),
OR(TEXT(  amc__Milestone__r.amc__Project__r.amc__Project_Day_Rate__c  ) = &apos;Skill-based&apos;,
TEXT(  amc__Milestone__r.amc__Project__r.amc__Cost_Rate__c   ) = &apos;Skill-based&apos;
))</errorConditionFormula>
        <errorDisplayField>amc__Skill__c</errorDisplayField>
        <errorMessage>Skill must be populated for Projects that use Skill-based costings.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>amc__Start_Time_Validation</fullName>
        <active>true</active>
        <description>If Event is checked, the Start Hour and Minute must be populated.</description>
        <errorConditionFormula>AND(amc__Event__c, OR(ISPICKVAL(amc__Start_Hour__c, &quot;&quot;), ISPICKVAL(amc__Start_Minute__c, &quot;&quot;)))</errorConditionFormula>
        <errorMessage>Start Hour and Start Minute are required to create an Event for this Action</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>amc__Task_or_Event_Creation</fullName>
        <active>true</active>
        <description>You can only create a Task or an Event from an Action, not both.</description>
        <errorConditionFormula>AND( amc__Event__c = TRUE,  amc__Task__c = TRUE)</errorConditionFormula>
        <errorDisplayField>amc__Task__c</errorDisplayField>
        <errorMessage>You can only create a Task or an Event from an Action, noth both.</errorMessage>
    </validationRules>
