        <fullName>Check_ES_VAT_Number</fullName>
        <active>true</active>
        <description>IF the customer is from Spain, the VAT Number must start with ES.</description>
        <errorConditionFormula>AND( Account__r.BillingCountry = &apos;ES&apos;,NOT(BEGINS(Account__r.VAT_Number__c, &apos;ES&apos;)))</errorConditionFormula>
        <errorMessage>VAT Number must start with &apos;ES&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EU_VAT_Validation</fullName>
        <active>true</active>
        <description>VAT amount is mandatory, if the customer is from EU</description>
        <errorConditionFormula>AND( 
CONTAINS(&quot;AD:AM:AT:BY:BE:BA:BG:CH:CY:CZ:DE:DK:EE:ES:FO:FI:FR:GB:GI:GR:HU:HR:IE:IS:IT:LT:LU:LV:MC:MT:NO:NL:PO:PT:RO:RU:SE:SI:SK:SM:FR:VA&quot;, Account__r.BillingCountry),
ISBLANK(Account__r.VAT_Number__c ))</errorConditionFormula>
        <errorMessage>VAT amount is mandatory, if the customer is from EU</errorMessage>
    </validationRules>
