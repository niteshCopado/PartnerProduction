        <formula>IF($Setup.copado__Settings__c.copado__Namespace__c != &apos;none&apos;,
IF(
TEXT(copado__Back_Promotion_Behavior__c) == &apos;Scheduled&apos;, 
HYPERLINK(&quot;/apex/copado__ScheduleConnectionBehavior?id=&quot; + Id + &quot;&amp;promotionType=Back_Promotion&quot;, IF(   ISBLANK(copado__Back_Promotion_Schedule__c), &quot;Schedule Back-Promotion&quot;, copado__Back_Promotion_Schedule__c),&apos;_self&apos;),
&apos;&apos;),
