    <fields>
        <fullName>Bread_Winner__Status_Flag__c</fullName>
        <deprecated>false</deprecated>
        <description>Representing statuses in images</description>
        <externalId>false</externalId>
        <formula>IF(CONTAINS($User.UIThemeDisplayed, &apos;Theme4&apos;),
  IF(OR(Bread_Winner__Status__c = &apos;VOIDED&apos;, Bread_Winner__Status__c = &apos;DELETED&apos;),
    IMAGE(&quot;https://s3.amazonaws.com/bw-cdn/x-ltng.png&quot;, &quot;Deleted / Voided&quot;),
    IF(
      OR(Bread_Winner__Type__c==&apos;Credit Note (Invoice)&apos;, Bread_Winner__Type__c==&apos;Invoice&apos;),
      IF( 
        OR(Bread_Winner__Status__c = &apos;DRAFT&apos;, Bread_Winner__Status__c = &apos;SUBMITTED&apos;),
        IMAGE(&quot;https://s3.amazonaws.com/bw-cdn/pencil.png&quot;, &quot;Draft / Submitted for Approval&quot; ) ,
        IF(Bread_Winner__Amount_Overdue__c &gt; 0,
          IMAGE(&quot;https://s3.amazonaws.com/bw-cdn/red-flag.png&quot;, &quot;Overdue&quot;) ,
          IF( Bread_Winner__Amount_Due__c &gt; 0,
            IMAGE(&quot;https://s3.amazonaws.com/bw-cdn/yellow-flag.png&quot;, &quot;Due&quot;) ,
            IF(AND(Bread_Winner__Type__c == &apos;Credit Note (Invoice)&apos;,Bread_Winner__Credit_Note_Allocated_Credit__c  &lt;&gt; Bread_Winner__Credit_Note_Total_Amount__c) ,
              IMAGE(&quot;https://s3.amazonaws.com/bw-cdn/yellow-flag.png&quot;, &quot;Due&quot;),
              IMAGE(&quot;https://s3.amazonaws.com/bw-cdn/green-flag.png&quot;, &quot;Paid&quot;)
            )))
      ),      
      IF(AND(OR(Bread_Winner__Type__c==&apos;Credit Note (Bill)&apos;, Bread_Winner__Type__c==&apos;Bill&apos;),Bread_Winner__Status__c!=&apos;DRAFT&apos;),
        IF(OR(Bread_Winner__Status__c != &apos;PAID&apos;,Bread_Winner__Amount_Due__c &gt; 0,Bread_Winner__Amount_Overdue__c &gt; 0),IF(Bread_Winner__Amount_Overdue__c&gt;0,IMAGE(&quot;https://s3.amazonaws.com/cdn.breadwinner/bills_r.png&quot;, &quot;Overdue&quot;),IMAGE(&quot;https://s3.amazonaws.com/cdn.breadwinner/bills_o.png&quot;, &quot;Due&quot;)),IMAGE(&quot;https://s3.amazonaws.com/cdn.breadwinner/bills_g.png&quot;, &quot;Paid&quot;)
            ),
        IF(Bread_Winner__Status__c == &apos;DRAFT&apos;,IMAGE(&quot;https://s3.amazonaws.com/bw-cdn/pencil.png&quot;, &quot;Draft&quot; ),
           IF(OR(Bread_Winner__Status__c == &apos;SUBMITTED&apos;, Bread_Winner__Status__c == &apos;AUTHORISED&apos;),IMAGE(&quot;https://s3.amazonaws.com/cdn.breadwinner/po_o.png&quot;, &quot;SUBMITTED/AUTHORISED &quot;),IF(OR(Bread_Winner__Status__c == &apos;BILLED&apos;),IMAGE(&quot;https://s3.amazonaws.com/cdn.breadwinner/po_g.png&quot;, &quot;BILLED&quot;),IMAGE(&quot;https://s3.amazonaws.com/bw-cdn/pencil.png&quot;, &quot;Draft&quot; ) ) ) ) ) )   
  ),
  IF( 
   OR(Bread_Winner__Status__c = &apos;VOIDED&apos;, Bread_Winner__Status__c = &apos;DELETED&apos;),   
   IMAGE(&quot;https://s3.amazonaws.com/bw-cdn/x-cls.png&quot;, &quot;Deleted / Voided&quot;,16,16),      
    IF( OR(Bread_Winner__Type__c==&apos;Credit Note (Invoice)&apos;, Bread_Winner__Type__c==&apos;Invoice&apos;),
      IF(OR(Bread_Winner__Status__c = &apos;DRAFT&apos;, Bread_Winner__Status__c = &apos;SUBMITTED&apos;),
        IMAGE(&quot;https://s3.amazonaws.com/bw-cdn/pencil.png&quot;, &quot;Draft / Submitted for Approval&quot;, 16, 16) ,
        IF( Bread_Winner__Amount_Overdue__c &gt; 0,
          IMAGE(&quot;/img/samples/flag_red.gif&quot;, &quot;Overdue&quot;) ,
          IF( Bread_Winner__Amount_Due__c &gt; 0,
            IMAGE(&quot;/img/samples/flag_yellow.gif&quot;, &quot;Due&quot;) ,
            IF(AND(Bread_Winner__Type__c == &apos;Credit Note (Invoice)&apos;,Bread_Winner__Credit_Note_Allocated_Credit__c  &lt;&gt; Bread_Winner__Credit_Note_Total_Amount__c), IMAGE(&quot;/img/samples/flag_yellow.gif&quot;, &quot;Due&quot;), IMAGE(&quot;/img/samples/flag_green.gif&quot;, &quot;Paid&quot;))))),
      IF(AND(OR(Bread_Winner__Type__c==&apos;Credit Note (Bill)&apos;, Bread_Winner__Type__c==&apos;Bill&apos;),Bread_Winner__Status__c!=&apos;DRAFT&apos;),
        IF(OR(Bread_Winner__Status__c != &apos;PAID&apos;,Bread_Winner__Amount_Due__c &gt; 0,Bread_Winner__Amount_Overdue__c &gt; 0),IF(Bread_Winner__Amount_Overdue__c&gt;0,IMAGE(&quot;https://s3.amazonaws.com/cdn.breadwinner/bills_r.png&quot;, &quot;Overdue&quot;,18,18),IMAGE(&quot;https://s3.amazonaws.com/cdn.breadwinner/bills_o.png&quot;, &quot;Due&quot;,18,18)),IMAGE(&quot;https://s3.amazonaws.com/cdn.breadwinner/bills_g.png&quot;, &quot;Paid&quot;,18,18)),
        IF(Bread_Winner__Status__c == &apos;DRAFT&apos;,IMAGE(&quot;https://s3.amazonaws.com/bw-cdn/pencil.png&quot;,&quot;Draft&quot;,16,16),IF(Bread_Winner__Status__c == &apos;BILLED&apos;,IMAGE(&quot;https://s3.amazonaws.com/cdn.breadwinner/po_g.png&quot;, &quot;Billed&quot;,18,18),IMAGE(&quot;https://s3.amazonaws.com/cdn.breadwinner/po_o.png&quot;,&quot;SUBMITTED&quot;,18,18)))
       ))))</formula>
