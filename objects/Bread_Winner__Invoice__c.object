    <fields>
        <fullName>Bread_Winner__Invoice_URL__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>if( CONTAINS(Bread_Winner__Type__c, &apos;Purchase Order&apos;),
	HYPERLINK( &quot;https://go.xero.com/Accounts/Payable/PurchaseOrders/Edit/&quot;&amp;Bread_Winner__Invoice_Id__c, 
		&quot;Click Here&quot;
	),
	if(CONTAINS(Bread_Winner__Type__c, &apos;Invoice&apos;),
		HYPERLINK( &quot;https://go.xero.com/AccountsReceivable/&quot;&amp; 
		IF(Bread_Winner__Type__c == &apos;Credit Note (Invoice)&apos;,
			&quot;ViewCreditNote.aspx?creditNoteID=&quot;,
			IF(Bread_Winner__Status__c == &apos;DRAFT&apos;,
				&quot;Edit.aspx?invoiceID=&quot;,
				&quot;View.aspx?invoiceID=&quot;
			)
		)
		&amp;Bread_Winner__Invoice_Id__c,
		&quot;Click Here&quot;
		), 
		HYPERLINK( &quot;https://go.xero.com/AccountsPayable/&quot;&amp; 
		IF(Bread_Winner__Type__c == &apos;Credit Note (Bill)&apos;,
			&quot;ViewCreditNote.aspx?creditNoteID=&quot;,
			IF(Bread_Winner__Status__c == &apos;DRAFT&apos;,
				&quot;Edit.aspx?invoiceID=&quot;,
				&quot;View.aspx?invoiceID=&quot;
			)
		)
		&amp;Bread_Winner__Invoice_Id__c,
		&quot;Click Here&quot;
		)
	)
)</formula>
        <label>See this Invoice in Xero</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
