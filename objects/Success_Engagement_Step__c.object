        <type>Date</type>
    </fields>
    <fields>
        <fullName>Is_Open__c</fullName>
        <externalId>false</externalId>
        <formula>NOT(
 OR(
  TEXT(Status__c)==&apos;Completed&apos;,
  TEXT(Status__c)==&apos;Cancelled&apos;
 )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>TRUE if the Status is Completed or Cancelled.</inlineHelpText>
        <label>Is Open</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Is_Overdue__c</fullName>
        <externalId>false</externalId>
        <formula>AND(
 Due_Date__c &lt; TODAY(),
 Is_Open__c==true
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>TRUE If the Due Date is greater than Today and Step is Open.</inlineHelpText>
        <label>Is Overdue</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
