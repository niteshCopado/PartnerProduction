    <validationRules>
        <fullName>kugo2p__AdditionalProductDetailLocked</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED( kugo2p__ReferenceProduct__c )</errorConditionFormula>
        <errorDisplayField>kugo2p__ReferenceProduct__c</errorDisplayField>
        <errorMessage>This Field Cannot Be Changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>kugo2p__KitBundleCheck</fullName>
        <active>true</active>
        <description>A Kit/Bundle requires a pricing method</description>
        <errorConditionFormula>AND(
  OR(
    kugo2p__KitBundle__c = true,
    TEXT(kugo2p__ConfigurationMethod__c) = &apos;Kit/Bundle&apos;
  ),
  Text(kugo2p__KitBundlePricing__c) = &quot;&quot;
)</errorConditionFormula>
        <errorDisplayField>kugo2p__KitBundlePricing__c</errorDisplayField>
        <errorMessage>Please Pick a Kit/Bundle Pricing Method.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>kugo2p__MaximumQuantityDefaultCheck</fullName>
        <active>true</active>
        <errorConditionFormula>kugo2p__MaximumQuantity__c  &lt; kugo2p__DefaultQuantity__c</errorConditionFormula>
        <errorDisplayField>kugo2p__MaximumQuantity__c</errorDisplayField>
        <errorMessage>Maximum Quantity must be greater than or equal to Default Quantity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>kugo2p__MaximumQuantityMinimumCheck</fullName>
        <active>true</active>
        <errorConditionFormula>kugo2p__MaximumQuantity__c  &lt;  kugo2p__MinimumQuantity__c</errorConditionFormula>
        <errorDisplayField>kugo2p__MaximumQuantity__c</errorDisplayField>
        <errorMessage>Maximum Quantity must be greater than or equal to Minimum Quantity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>kugo2p__MaximumQuantityZeroCheck</fullName>
        <active>true</active>
        <errorConditionFormula>kugo2p__MaximumQuantity__c  &lt;= 0</errorConditionFormula>
        <errorDisplayField>kugo2p__MaximumQuantity__c</errorDisplayField>
        <errorMessage>Maximum Quantity must be greater than Zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>kugo2p__Percent_of_Price_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(kugo2p__PercentOfUnitPrice__c  &gt; 0,   ISPICKVAL(kugo2p__PercentofPriceBasedon__c,&quot;&quot;)  )</errorConditionFormula>
        <errorDisplayField>kugo2p__PercentofPriceBasedon__c</errorDisplayField>
        <errorMessage>Please Choose Price Type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>kugo2p__ServiceBasicCheck</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
 kugo2p__Service__c  = true, 
Text( kugo2p__UnitofTerm__c )=&quot;&quot;
)</errorConditionFormula>
        <errorDisplayField>kugo2p__UnitofTerm__c</errorDisplayField>
        <errorMessage>Service requires a Unit of Term. Please Pick a Unit of Term.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>kugo2p__VATCheck</fullName>
        <active>true</active>
        <description>VAT Must Be Picked, If Taxable VAT is Checked</description>
        <errorConditionFormula>AND (
  kugo2p__TaxableVAT__c = true, 
  kugo2p__VAT__c=&quot;&quot;
)</errorConditionFormula>
        <errorDisplayField>kugo2p__VAT__c</errorDisplayField>
        <errorMessage>Please Choose VAT.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
