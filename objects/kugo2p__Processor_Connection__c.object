    <webLinks>
        <fullName>kugo2p__Test_Relay_Script</fullName>
        <availability>online</availability>
        <description>Runs a connection test to ensure that Kugamon and the relay script for this connection are configured correctly.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Test Relay Script</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/18.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/18.0/apex.js&quot;)}

function isNullOrEmpty(value) {
    if (value == null) return true;
    if (value == &apos;&apos;) return true;
    return false;
}

function runtest() {

  if (&apos;{!kugo2p__Processor_Connection__c.kugo2p__Notification_Relay_Method__c}&apos; == &apos;&apos;) {
     alert(&apos;Please select a notification relay method for this processor connection.&apos;);
     return;
  }
  if (isNullOrEmpty(&apos;{!kugo2p__Processor_Connection__c.kugo2p__Relay_Script__c}&apos;)) {
     alert(&apos;A relay script URL has not been provided.  Please make sure you have installed and configured a Kugamon relay for this processor connection and enter the URL in the processor connection settings record.&apos;);
     return;
  }

  var result = sforce.apex.execute(&apos;kugo2p/PaymentX&apos; ,&apos;testNotificationHandler&apos;,{processor:&apos;{!kugo2p__Processor_Connection__c.Id}&apos;}, null, false);
  var resultstr = (result == null?&quot;&quot;:result.toString());
  var resultmsg = resultstr;
  if (resultstr.search(/(&quot;|&apos;)*success(&quot;|&apos;)*:/i) &gt; -1)  {
      var resultObj = !(/[^,:{}\[\]0-9.\-+Eaeflnr-u \n\r\t]/.test(resultstr.replace(/&quot;(\\.|[^&quot;\\])*&quot;/g, &apos;&apos;))) &amp;&amp; eval(&apos;(&apos; + resultstr + &apos;)&apos;);
      if (resultObj.success) {
            resultmsg = &apos;A test notification has been sent to your relay script.  If the script has been installed and configured correctly, you will see a completed activity below indicating a successful reply.  This test may take a few minutes if your network is busy.&apos;;
     }	
    if (resultObj !=null &amp;&amp; resultObj.success == false &amp;&amp; resultObj.message != null) {
       resultmsg = &quot;Relay Script Error: &quot;+resultObj.message;
    }
  }
  if (resultstr.search(/Page not found/i) &gt; -1) resultmsg = &apos;The relay script could not be found.  Please check the URL.&apos;;
  alert (resultmsg);
  window.location.reload();
  return;
}

runtest();</url>
    </webLinks>
