NOT(ISNULL({!myVariable_current.Minutes__c})),
NOT(ISNULL({!myVariable_current.Case__c})) ,
ISPICKVAL({!myVariable_current.Type__c} ,&quot;Time Logging&quot;)),
AND(
ISCHANGED({!myVariable_current.Minutes__c} ),
NOT(ISNULL({!myVariable_current.Case__c})) ,
ISPICKVAL({!myVariable_current.Type__c} ,&quot;Time Logging&quot;)
