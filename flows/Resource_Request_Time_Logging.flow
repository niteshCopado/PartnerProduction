    <description>This process will populate the duration of each request from the Meeting Records</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>IF(
[Demo__c].Case__r.Hours_Actual__c=0,
([Demo__c].Minutes__c/60),
[Demo__c].Case__r.Hours_Actual__c)
+
IF
(AND([Demo__c].Minutes__c=PRIORVALUE([Demo__c].Minutes__c),[Demo__c].Case__r.Hours_Actual__c!=0),([Demo__c].Minutes__c/60),
(([Demo__c].Minutes__c-PRIORVALUE([Demo__c].Minutes__c))/60))


</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_2_myRule_1_A1_2649827935</name>
        <dataType>Number</dataType>
        <expression>IF(
{!myVariable_current.Case__r.Hours_Actual__c}=0,
({!myVariable_current.Minutes__c}/60),
{!myVariable_current.Case__r.Hours_Actual__c})
+
IF
(AND({!myVariable_current.Minutes__c}=PRIORVALUE({!myVariable_current.Minutes__c}),{!myVariable_current.Case__r.Hours_Actual__c}!=0),({!myVariable_current.Minutes__c}/60),
(({!myVariable_current.Minutes__c}-PRIORVALUE({!myVariable_current.Minutes__c}))/60))</expression>
        <scale>18</scale>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>OR(
AND(
ISNEW(),
NOT(ISNULL([Demo__c].Minutes__c)),
NOT(ISNULL([Demo__c].Case__c)) ,
ISPICKVAL([Demo__c].Type__c ,&quot;Time Logging&quot;)),
AND(
ISCHANGED([Demo__c].Minutes__c ),
NOT(ISNULL([Demo__c].Case__c)) ,
ISPICKVAL([Demo__c].Type__c ,&quot;Time Logging&quot;)
) 
)
</stringValue>
